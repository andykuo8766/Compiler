void main() 
{
	int num1 = 0;
	float num2 = 0.0;
	float result = 0.0;
	
	printf("Please enter a number1(int):");
	scanf("%d", &num1);
	printf("Please enter a number2(float):");
	scanf("%f", &num2);
	if (num1 >= 10) {
		 result = num1 / num2;
	} 
	else {
		  result = num1 * num2;
	}
	printf("The number1: is %d\n", num1);
	printf("The number2: is %f\n", num2);
	printf("The result: is %f\n", result);
	
	
	
	printf("Please enter a number1(int) again:");
	scanf("%d", &num1);
	printf("Please enter a number2(float) again:");
	scanf("%f", &num2);
	if (num1 <= 10) {
		 result = num1 / num2;
	} 
	else {
		  result = num1 * num2;
	}
	printf("The number1: is %d\n", num1);
	printf("The number2: is %f\n", num2);
	printf("The result: is %f\n", result);	
	

}