void main() 
{
	int num;
	float s;
	
	printf("Please enter a number:");
	scanf("%d", &num);
	if (num > 10) {
		s = 3 * (num + 3.14);
	} else {
		s = num * (num - 3.14);
	}
	printf("The number: is %d\n", num);
	printf("The result is %f\n", s);
	
	printf("Please enter a number again:");
	scanf("%d", &num);
	if (num < 10) {
		s = 3 * (num + 3.14);
	} else {
		s = num * (num - 3.14);
	}
	printf("The number: is %d\n", num);
	printf("The result is %f\n", s);


}
